{

  "configurations": {
    "zootler": {
      "adapter": "vscode-go",
      "configuration": {
        "request": "launch",
        "program": "${workspaceRoot}/cmd/zootler/",
        "args": [
          "-d",
          "${workspaceRoot}/inputs/data/",
          "-l",
          "${workspaceRoot}/inputs/logic/glitchless"
        ],
        "mode": "debug",
        "dlvToolPath": "$GOPATH/bin/dlv"
      }
    },
    "test": {
      "adapter": "vscode-go",
      "configuration": {
        "request": "launch",
        "program": "${fileDirname}",
        "args": [],
        "mode": "test",
        "dlvToolPath": "$GOPATH/bin/dlv"
      }
    }
  }
}
